{
  "name": "mui-app-dir-demo",
  "version": "1.4.0",
  "engines": {
    "node": ">=18"
  },
  "description": "@mui with next.js App Router Demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/Howard86/mui-app-dir-demo"
  },
  "author": "Howard Tai <howard@howardism.dev>",
  "license": "MIT",
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "jest",
    "eslint",
    "prettier",
    "lint-staged",
    "husky",
    "commitlint"
  ],
  "bugs": {
    "url": "https://github.com/Howard86/mui-app-dir-demo/issues"
  },
  "homepage": "https://github.com/Howard86/mui-app-dir-demo",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules coverage tsconfig.tsbuildinfo",
    "check-all": "yarn type-check && yarn lint && yarn format",
    "type-check": "tsc --noEmit",
    "lint": "TIMING=1 eslint .",
    "format": "prettier --write --list-different .",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "next": "^13.5.5",
    "next-api-handler": "^0.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@next/bundle-analyzer": "^13.5.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-redux": "^7.1.27",
    "eslint": "^8.51.0",
    "eslint-config-howardism": "^0.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^15.0.1",
    "msw": "^1.3.2",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^2.0.1",
    "typescript": "^5.2.2",
    "whatwg-fetch": "^3.6.19"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "pnpm@8.7.0"
}
